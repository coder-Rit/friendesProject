                  <option value="Music Concerts">Music Concerts</option>
                  <option value="Gala Dinner">Gala Dinner</option>
                  <option value="Luxury Events">Luxury Events</option>
                  <option value="Luxury Stalls">Luxury Stalls</option>
                  <option value="Corporate Events">Corporate Events</option>
                </select>
              </div>
              <textarea name="message" id="msg" cols="30" rows="10" placeholder="Write your message here.....">
              </textarea>
              <button class="btn1">Submit</button>
            </form>
          </div>
        </div>
